<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context="com.example.jberger.moonwalker.MapsActivity">

    <TextView
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Congratulations!"
        android:layout_gravity="center"
        android:textColor="@android:color/black"
        android:shadowColor="@android:color/black"
        android:id="@+id/congratulations" />

    <TextView
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="gtfmjgtjhg j jhz6h j6zjzhljz 6l hilzi inz6ni z"
        android:layout_gravity="center"
        android:textColor="@android:color/black"
        android:shadowColor="@android:color/black"
        android:id="@+id/achivement" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/stones"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:context="com.example.jberger.moonwalker.MapsActivity">

        <ImageView
            android:id="@+id/a11"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:padding="16dp"
            android:src="@drawable/apollo11_batch"
            android:clickable="true" />

        <ImageView
            android:id="@+id/a12"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:padding="16dp"
            android:src="@drawable/apollo12_batch"
            android:clickable="true" />

        <ImageView
            android:id="@+id/a14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:padding="16dp"
            android:src="@drawable/apollo14_batch"
            android:clickable="true" />

        <ImageView
            android:id="@+id/a15"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:padding="16dp"
            android:src="@drawable/apollo15_batch"
            android:clickable="true" />

        <ImageView
            android:id="@+id/a16"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:padding="16dp"
            android:src="@drawable/apollo16_batch"
            android:clickable="true" />

        <ImageView
            android:id="@+id/a17"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:padding="16dp"
            android:src="@drawable/apollo17_batch"
            android:clickable="true" />
    </LinearLayout>
</LinearLayout>